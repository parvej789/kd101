#<src>                                             <mnt_point>            <type>        <mnt_flags and options>                                                                  <fs_mgr_flags>
system_a                                             /system              ext4                ro                                                                            wait,avb=vbmeta_system,first_stage_mount
system_ext_a                                         /system_ext          ext4                ro                                                                            wait,avb=first_stage_mount
vendor_a                                             /vendor              ext4                ro                                                                            wait,avb,first_stage_mount,
product_a                                            /product             ext4                ro                                                                            wait,avb,first_stage_mount,

/dev/block/platform/bootdevice/by-name/md_udc /metadata ext4 noatime,nosuid,nodev,discard wait,check,formattable,first_stage_mount
/dev/block/platform/bootdevice/by-name/userdata /data f2fs noatime,nosuid,nodev,discard,noflush_merge,reserve_root=134217,resgid=1065,inlinecrypt,tran_gc wait,check,formattable,quota,latemount,resize,reservedsize=128m,checkpoint=fs,fileencryption=aes-256-xts:aes-256-cts:v2,keydirectory=/metadata/vold/metadata_encryption
                                                                   defaults
/product                ext4        /dev/block/mapper/product_a                             flags=backup=1;display="Product ext4"
/product_image 	        emmc		/dev/block/mapper/product_a                             flags=backup=1;display="Product Image";flashimg=1
/super          	    emmc    	/dev/block/platform/bootdevice/by-name/super            flags=backup=1;display="Super_image";flashimg=1
/system      	        ext4		/dev/block/mapper/system_a                              flags=backup=1;display="System ext4"
/system_image           emmc		/dev/block/mapper/system_a                              flags=backup=1;display="System Image";flashimg=1
/system_ext_image       emmc		/dev/block/mapper/system_ext_a	                        flags=backup=1;display="system ext Image";flashimg=1
/vendor                 ext4        /dev/block/mapper/vendor_a                              flags=backup=1;display="Vendor ext4"
/vendor_image	        emmc		/dev/block/mapper/vendor_a                              flags=backup=1;display="Vendor Image";flashimg=1

# Other partitions

  
