/vbmeta 	     emmc	 /dev/block/platform/bootdevice/by-name/vbmeta_a               flags=display="vbmeta";backup=1;flashimg=1
/vbmeta_system   emmc    /dev/block/platform/bootdevice/by-name/vbmeta_system_a      flags=backup=1;display="vbmeta_system";flashimg=1
